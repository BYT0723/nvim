{
  "emoji": {
    "prefix": "emoji",
    "body": ["{% emoji $name %}"],
    "description": "hexo emoji"
  },
  "checkMark": {
    "prefix": "checkMark",
    "body": ["{% emoji heavy_check_mark %}$0"],
    "description": "emoji check mark heavy"
  },
  "linkWithUrl": {
    "prefix": "linkurl",
    "body": ["[{% emoji link %}]($1)$0"],
    "description": "link url"
  },
  "indent": {
    "prefix": "indent",
    "body": ["&emsp;&emsp;"],
    "description": "indent"
  },
  "split-line": {
    "prefix": "more",
    "body": ["<!--more-->"],
    "description": "split line"
  },
  "detail": {
    "prefix": "detail",
    "body": ["<details>", "\t<summary>$1</summary>", "\t$2", "</details>"],
    "description": "detail"
  },
  "note": {
    "prefix": "note",
    "body": [
      "- [ ] ${1|Work,Person,Home,Code,Other|} ${2:${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}}, ${3:description}"
    ],
    "description": "personal note"
  }
}
