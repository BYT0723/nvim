{
    "jsp head": {
        "prefix": "jsp",
        "body": [
            "<%@ page language=\"java\" contentType=\"text/html; charset=UTF-8\" pageEncoding=\"UTF-8\" %>",
            "<%@ page isELIgnored=\"false\" %>"
        ],
        "description": "jsp be required tag"
    },
    "import": {
        "prefix": "im",
        "body": [
            "<%@ page import=\"$1\" %>$0"
        ],
        "description": "import java class"
    },
    "contextPath": {
        "prefix": "cp",
        "body": [
            "${pageContext.request.contextPath}/"
        ],
        "description": "context path"
    },
    "jcblock": {
        "prefix": "jcb",
        "body": [
            "<%",
            "\t$1",
            "%>$0"
        ],
        "description": "java code block"
    },
    "jcvalue": {
        "prefix": "jcv",
        "body": [
            "<%=$1%>$0"
        ],
        "description": "jcv"
    },
    "el": {
        "prefix": "el",
        "body": [
            "${$1}$0"
        ],
        "description": "el style"
    }
}
